{
  "name": "timeline",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://timeline.infinitex.cn",
  "dependencies": {
    "@types/react-bootstrap": "^0.32.14",
    "bootstrap": "^4.1.3",
    "lrz": "^4.9.40",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-bootstrap": "^1.0.0-beta.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "source-map-explorer": "^1.6.0",
    "superagent": "^4.0.0",
    "superagent-promise": "^1.1.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
